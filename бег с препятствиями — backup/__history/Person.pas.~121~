  Frames[31].legl[3] := Point(Round(1220 * ScaleX), Round(1330 * ScaleY));

  Frames[31].legr[1] := Point(Round(1280 * ScaleX), Round(1000 * ScaleY));
  Frames[31].legr[2] := Point(Round(1370 * ScaleX), Round(1180 * ScaleY));
  Frames[31].legr[3] := Point(Round(1300 * ScaleX), Round(1340 * ScaleY));

  Frames[31].head := Rect(Round(1300 * ScaleX), Round(640 * ScaleY), Round(1440 * ScaleX), Round(500 * ScaleY));
  Frames[31].body[1] := Point(Round(1280 * ScaleX), Round(1000 * ScaleY));
  Frames[31].body[2] := Point(Round(1380 * ScaleX), Round(540 * ScaleY));

  Frames[31].pmax := Point(Round(1500 * ScaleX), Round(1340 * ScaleY));

end;

end.

